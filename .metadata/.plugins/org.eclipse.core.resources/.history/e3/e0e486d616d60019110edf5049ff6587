package com.pixogram.media.controller;

/*import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;
import lombok.ToString;*/

import javax.persistence.*;

@Entity
@Table(name="image_model")
/*
 * @AllArgsConstructor
 * 
 * @Data
 * 
 * @NoArgsConstructor
 * 
 * @ToString
 */
public class ImageModel {
    @Id
    @Column(name = "id")
    @GeneratedValue
    private Long id;

    @Column(name = "name")
    private String name;

    @Column(name = "type")
    private String type;
    @Column(name = "username")
    private String username;

    @Lob
    @Column(name = "pic")
    private byte[] pic;

public ImageModel() {
		super();
	}

	//Custom Construtor
    public ImageModel(String name, String type,String username ,byte[] pic) {
       this.username=username;
    	this.name = name;
        this.type = type;
        this.pic = pic;
    }
}